generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Pedidos{
  id_client Int @id @default(autoincrement())
  cep_origem String
  cep_destino String
  telefone String
  nome_solicitante String
}